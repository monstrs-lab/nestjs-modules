{
  "name": "@monstrs/nestjs-kafka",
  "version": "0.0.6",
  "license": "MIT",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn library build",
    "prepack": "yarn run build",
    "postpack": "rm -rf dist"
  },
  "dependencies": {
    "@monstrs/logger": "^0.0.20",
    "camelcase": "^8.0.0",
    "kafkajs": "^2.2.4"
  },
  "devDependencies": {
    "@nestjs/common": "^10.0.5",
    "@nestjs/core": "^10.0.5",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "peerDependencies": {
    "@nestjs/common": "^10",
    "@nestjs/core": "^10",
    "reflect-metadata": "^0.2",
    "rxjs": "^7"
  },
  "publishConfig": {
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "main": "dist/index.js",
    "typings": "dist/index.d.ts"
  }
}
